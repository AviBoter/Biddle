<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.biddle">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Biddle">
        <activity android:name=".Activites.EditProfileCustomerActivity"></activity>
        <activity android:name=".Activites.EditProfileSellerActivity" />
        <activity android:name=".Activites.PurchasedProductsSellerActivity" />
        <activity android:name=".Activites.SellerDetailsActivity" />

        <activity android:name=".Activites.PurchasedProductsCustomerActivity" />
        <activity android:name=".Activites.PriceOfferedProductsCustomerActivity" />
        <activity android:name=".Activites.StarProductsCustomerActivity" />
        <activity android:name=".Activites.ProductDetailsActivity" />
        <activity
            android:name=".Activites.CustomerActivity"
            android:label="@string/title_activity_customer" />
        <activity
            android:name=".Activites.ProductFormActivity"
            android:label="@string/title_activity_product_form"
            android:theme="@style/Theme.Biddle.NoActionBar" />
        <activity android:name=".Activites.SellerActivity" />
        <activity
            android:name=".Activites.LandingPageActivity"
            android:label="@string/title_activity_landing_page"
            android:theme="@style/Theme.Biddle.NoActionBar" />
        <activity
            android:name=".Activites.SignupActivity"
            android:label="@string/title_activity_signup"
            android:theme="@style/Theme.Biddle.NoActionBar" />
        <activity android:name=".Activites.MainActivity">
            <intent-filter>
                <category android:name="android.intent.category.LAUNCHER" />

                <action android:name="android.intent.action.MAIN" />
            </intent-filter>
        </activity>
    </application>

</manifest>